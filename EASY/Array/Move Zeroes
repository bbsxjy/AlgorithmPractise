public class Solution {
    public void moveZeroes(int[] nums) {
        //2 loops
    	int i=0;
    	int j=nums.length-1;
    	while(i!=j){
    		if(nums[i]==0){
    			for(int k=i;k<nums.length-1;k++){
    				nums[k]=nums[k+1];
    			}
    			nums[j]=0;
    			j--;
    		}else{
    			i++;
    		}
    		
    		
    		
    	}
    	
    
    }
}
