public class Solution {
    public int[] twoSum(int[] nums, int target) {
        int [] thePick=new int[2];
        
        
        for(int i =0;i<nums.length;i++){
            int j=i+1;
            while(j<nums.length){
               if(nums[i]+nums[j]==target){
                thePick[0]=i;
                thePick[1]=j;
               }
               j++;
               
            }
            
        }
        return thePick;
    }
}
