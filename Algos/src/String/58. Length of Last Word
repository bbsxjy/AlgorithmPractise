public class Solution {
 public int lengthOfLastWord(String s) {
        if(s.length()==0){
        	return 0;
        }	
    	s=s.toLowerCase();
        String [] att=s.split(" ");
        
        
        if(att.length==0){
        	return 0;
        }
        else{
        	String temp=att[att.length-1];
        	return temp.length();
        }
        
    }
}
